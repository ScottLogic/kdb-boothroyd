{
  "name": "kdb-studio-2",
  "version": "0.0.2",
  "description": "GUI for KDB/q database",
  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack.react.config.js --mode development",
    "build:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
    "build:react": "NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "build": "npm run build:electron && npm run build:react",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "electron-builder --publish",
    "test:electron": "NODE_ENV=test webpack --config webpack.electron.config.js --mode production",
    "test:react": "NODE_ENV=test webpack --config webpack.react.config.js --mode production",
    "build:test": "npm run test:electron && npm run test:react",
    "test": "cross-env NODE_ENV=test mocha --require ./tests/hooks.js ./tests",
    "prepare": "husky install"
  },
  "keywords": [],
  "contributors": [
    "ColinEberhardt",
    "ihull",
    "oinutter"
  ],
  "license": "ISC",
  "build": {
    "files": [
      "dist/",
      "build/",
      "node_modules/",
      "package.json"
    ],
    "productName": "KDB Studio 2",
    "appId": "com.scottlogic.kdbstudio2",
    "directories": {
      "output": "dist"
    },
    "icon": "build/icons/icon.png",
    "publish": "github"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.14.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/crypto-js": "^4.0.2",
    "@types/electron-json-storage": "^4.5.0",
    "@types/uuid": "^8.3.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "electron": "^13.1.2",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.0",
    "mocha": "^9.0.3",
    "monaco-editor-webpack-plugin": "^4.1.1",
    "playwright": "^1.13.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-refresh": "^0.9.0",
    "style-loader": "^3.1.0",
    "webpack-cli": "^4.*",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@fluentui/react": "^8.*",
    "@fluentui/react-file-type-icons": "^8.2.1",
    "@react-hook/resize-observer": "^1.2.2",
    "@types/react": "^17.*",
    "@types/react-dom": "^17.*",
    "ag-grid-community": "^25.3.0",
    "ag-grid-react": "^25.3.0",
    "crypto-js": "^4.1.1",
    "electron-dl": "^3.2.1",
    "electron-json-storage": "^4.5.0",
    "electron-updater": "^4.3.9",
    "node-q": "2.x.x",
    "office-ui-fabric-core": "^11.0.0",
    "react": "^17.*",
    "react-dom": "^17.*",
    "react-hot-loader": "^4.13.0",
    "react-monaco-editor": "^0.43.0",
    "react-split": "^2.0.11",
    "uuid": "^3.4.0",
    "webpack": "^5.*",
    "xlsx": "^0.17.0"
  }
}
